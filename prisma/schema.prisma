datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model User {
  id    Int     @id @default(autoincrement())
  email String  @unique
  name  String?
  Datas Data[]
}

model Data {
  id        Int     @id @default(autoincrement())
  title     String
  user    User    @relation(fields: [userId], references: [id])
  userId  Int
}